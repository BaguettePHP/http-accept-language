{
    "name": "zonuexe/http-accept-language",
    "description": "HTTP Accept-Language Header parser",
    "license": "MIT",
    "authors": [
        {
            "name": "USAMI Kenta",
            "email": "tadsan@zonu.me"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "ext-intl": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^8.5|^7.5|^4.8",
        "yoast/phpunit-polyfills": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Teto\\HTTP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Teto\\HTTP\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
